[core]
	editor = vim

[user]
    name = LRDPRDX 
    email = paradox1859@gmail.com

[diff]
    tool = vimdiff

[alias]
    a   = add
    al  = add --all

    b   = branch

    c   = commit
    co  = checkout
    cob = checkout -b
    com = checkout master

    d   = diff
    dc  = diff --cached
    dk  = diff --check
    dck = diff --cached --check
    dt  = difftool
    dct = difftool --cached

    f = fetch

    l   = log
    lgo = log --oneline --graph
    lo  = log --oneline

    p   = push

    r = remote

    s = status

    t = tag
